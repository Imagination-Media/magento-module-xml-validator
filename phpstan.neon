parameters:
	level: 8
	fileExtensions:
		- php
	paths:
		- src
		- test
	excludePaths:
		analyse:
			- test/*/_stubs/*
	treatPhpDocTypesAsCertain: false
	reportUnmatchedIgnoredErrors: false
	ignoreErrors:
		- '#Call to static method getObjectManager\(\) on an unknown class Magento\\TestFramework\\Helper\\Bootstrap#'
		- '#constant BP not found\.#i'
		- '#^Class Magento\\Setup\\Model\\ObjectManagerProvider not found\.$#'
		- '#^Call to method [^(]+\(\) on an unknown class Magento\\Setup\\Model\\ObjectManagerProvider\.$#'
		- '#^Parameter \$objectManagerProvider of method [^(]+\(\) has invalid type Magento\\Setup\\Model\\ObjectManagerProvider\.$#'
